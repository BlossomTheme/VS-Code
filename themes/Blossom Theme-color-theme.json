            ],
            "settings": {
                "foreground": "#fd21ef"
            }
        },
        {
            "name": "JSON String",
            "scope": "meta.structure.dictionary.json string.quoted.double.json",
            "settings": {
                "foreground": "#FF0081"
            }
        },
        {
            "name": "JSON Key",
