    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "string.quoted.docstring.multi.python"
            ],
            "settings": {
                "foreground": "#01C8FF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                //"foreground": "#5994CE"
                "foreground": "#00ff7d"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#ffd400"
            }
        },
        {
            "name": "Built-in constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#f70047"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#c832ff"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": "",
                "foreground": "#76FFF9"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#ff2cf1"
                // "foreground": "#ffd400"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "fontStyle": "",
                "foreground": "#40a9ff"
            }
        },
        {
            "name": "Storage type",
            "scope": "storage.type",
            "settings": {
                "fontStyle": "",
                "foreground": "#f46e00"
            }
        },
        {
            "name": "Meta",
            "scope": "meta",
            "settings": {
                "foreground": "#FF058D"
            }
        },
        {
            "name": "Meta JSX",
            "scope": [
                "meta.jsx.children",
                "meta.jsx.children.js",
                "meta.jsx.children.tsx"
            ],
            "settings": {
                "foreground": "#ffd400"
            }
        },
        {
            "name": "Variable Other Object JSX",
            "scope": [
                "variable.other.object.js",
                "variable.other.object.jsx",
                "variable.object.property.js",
                "variable.object.property.jsx"
            ],
            "settings": {
                "foreground": "#40a9ff"
            }
        },
        {
            "name": "Punctuation Parameters",
            "scope": "punctuation.definition.parameters",
            "settings": {
                "foreground": "#ffee80"
            }
        },
        {
            "name": "Punctuation Template Expression",
            "scope": "punctuation.definition.template-expression",
            "settings": {
                "foreground": "#ffee80"
            }
        },
        {
            "name": "Class name",
            "scope": [
                "entity.name.class",
                "entity.name.type.class"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#8700D9"
            }
        },
        {
            "name": "Entity",
            "scope": "entity",
            "settings": {
                "foreground": "#ffd400"
            }
        },
        {
            "name": "Inherited class",
            "scope": [
                "entity.other.inherited-class",
                "meta.other.inherited-class.php"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#6ab0ff"
            }
        },
        {
            "name": "Function name",
            "scope": "entity.name.function - meta.function-call",
            "settings": {
                "fontStyle": "",
                "foreground": "#01C8FF"
            }
        },
        {
            "name": "Function argument",
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "",
                "foreground": "#FF058D"
            }
        },
        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "fontStyle": "",
                "foreground": "#FF058D"
            }
        },
        {
            "name": "Tag attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "fontStyle": "",
                "foreground": "#ffd400"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#ffd400"
            }
        },
        {
            "name": "Library constant",
            "scope": "support.constant",
            "settings": {
                "fontStyle": "",
                "foreground": "#39c0ff"
            }
        },
        {
            "name": "Library class/type",
            "scope": [
                "support.type",
                "support.class"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#ff2cf1"
            }
        },
        {
            "name": "Library variable",
            "scope": "support.other.variable",
            "settings": {
                "fontStyle": "",
                "foreground": "#39c0ff"
            }
        },
        {
            "name": "Support Variable Property DOM",
